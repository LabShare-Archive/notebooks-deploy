# Add your oauth config to this file,
# and run the container with `docker run -it -p 9000:8000 --env-file=env jupyterhub-oauth`

OAUTH_CLIENT_ID=
OAUTH_CLIENT_SECRET=
ORGANIZATION=
AUTH_URL=https://a.labshare.org/_auth/vTest
OAUTH_CALLBACK_URL=https://127.0.0.1:9000/hub/oauth_callback

OAUTH2_AUTHORIZE_URL=$AUTH_URL/auth/$ORGANIZATION/authorize
OAUTH2_TOKEN_URL=$AUTH_URL/auth/$ORGANIZATION/oidc/token